{
  "dashboard": {
    "id": null,
    "title": "OmenDB Performance Dashboard",
    "tags": ["omendb", "database", "performance"],
    "timezone": "browser",
    "refresh": "10s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Database Operations Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(omendb_searches_total[5m])",
            "legendFormat": "Searches/sec"
          },
          {
            "expr": "rate(omendb_inserts_total[5m])",
            "legendFormat": "Inserts/sec"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 12,
          "x": 0,
          "y": 0
        },
        "fieldConfig": {
          "defaults": {
            "unit": "ops"
          }
        }
      },
      {
        "id": 2,
        "title": "Query Latency",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(omendb_search_duration_seconds_bucket[5m]))",
            "legendFormat": "50th percentile"
          },
          {
            "expr": "histogram_quantile(0.95, rate(omendb_search_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.99, rate(omendb_search_duration_seconds_bucket[5m]))",
            "legendFormat": "99th percentile"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        },
        "yAxes": [
          {
            "unit": "s"
          }
        ]
      },
      {
        "id": 3,
        "title": "Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(omendb_failed_searches_total[5m])",
            "legendFormat": "Search Errors/sec"
          },
          {
            "expr": "rate(omendb_failed_inserts_total[5m])",
            "legendFormat": "Insert Errors/sec"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 4
        },
        "yAxes": [
          {
            "unit": "ops"
          }
        ]
      },
      {
        "id": 4,
        "title": "Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "omendb_memory_usage_bytes",
            "legendFormat": "Memory Usage"
          }
        ],
        "gridPos": {
          "h": 6,
          "w": 8,
          "x": 0,
          "y": 12
        },
        "yAxes": [
          {
            "unit": "bytes"
          }
        ]
      },
      {
        "id": 5,
        "title": "Index Size",
        "type": "stat",
        "targets": [
          {
            "expr": "omendb_index_size",
            "legendFormat": "Index Keys"
          }
        ],
        "gridPos": {
          "h": 6,
          "w": 8,
          "x": 8,
          "y": 12
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      },
      {
        "id": 6,
        "title": "Database Size",
        "type": "stat",
        "targets": [
          {
            "expr": "omendb_database_size_bytes",
            "legendFormat": "Database Size"
          }
        ],
        "gridPos": {
          "h": 6,
          "w": 8,
          "x": 16,
          "y": 12
        },
        "fieldConfig": {
          "defaults": {
            "unit": "bytes"
          }
        }
      }
    ]
  }
}